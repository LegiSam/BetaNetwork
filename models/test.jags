
    model {
    
    #True State for each bird, plant, camera
    for(i in 1:Birds){
    for(j in 1:Plants){
      logit(s[i,j])<-alpha[i,j]
    }
    }
    
    #Observation Model
    for (x in 1:Nobs){
    Yobs[x] ~ dbern(s[Bird[x],Plant[x]])
    }
    
    #Priors
    
    for (i in 1:Birds){
    for (j in 1:Plants){
    #Intercept
    alpha[i,j] ~ dnorm(0,0.386)
    } 
    }
    
    }
    
